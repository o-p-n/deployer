{
  "imports": {
    "cliffy/": "https://deno.land/x/cliffy@v1.0.0-rc.3/",
    "dax": "https://deno.land/x/dax@0.36.0/mod.ts",
    "deno_std/": "https://deno.land/std@0.212.0/",
    "deno_std_209/": "https://deno.land/std@0.209.0/",
    "expecto/": "https://deno.land/x/expecto@v0.1.4/mod/"
  },
  "lint": {
    "exclude": [".cache/**"]
  },
  "fmt": {
    "exclude": [".cache/**"]
  },
  "tasks": {
    "checks": "deno lint && deno fmt --check",
    "cover": "git clean -dfx coverage && deno test -A --coverage && deno coverage --exclude=test --lcov --output=coverage/lcov coverage && deno coverage --exclude=test --html coverage && deno coverage --exclude=test coverage",
    "compile": "deno compile --allow-read --allow-write --allow-run --allow-env --output deployer src/main.ts"
  }
}
